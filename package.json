  "name": "leaflet-tracking",
  "version": "0.1",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist/leaflet-tracking",
    "ng": "ng",
    "start": "ng serve",
    "start-https": "ng serve --ssl true --ssl-key /etc/letsencrypt/live/szczepanik.cz/privkey.pem --ssl-cert /etc/letsencrypt/live/szczepanik.cz/fullchain.pem",
